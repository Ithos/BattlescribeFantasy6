                  <characteristics>
                    <characteristic name="M" typeId="2d56-9cad-9242-37bd">20</characteristic>
                    <characteristic name="HA" typeId="c87b-b820-6a47-1354">4</characteristic>
                    <characteristic name="HP" typeId="8ab7-c800-6644-f5ec">0</characteristic>
