            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="bf54-55bd-5e14-ca29" type="max"/>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="ad65-072d-256c-4482" type="max"/>
          </constraints>
          <profiles>
            <profile id="9b90-574e-d87c-cd81" name="Corazón de Ranu" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">Un único uso. El portador puede repetir un dado para lanzar o dispersar un hechizo. Esta habilidad puede usarse para evitar una disfunción mágica, para lanzar un hechizo con fuerza irresistible, etc.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
