    <selectionEntry id="a8e4-dc6d-b1f7-cd0e" name="Cráneo de la muerte" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="48e8-7c18-794e-929c" type="max"/>
