          </constraints>
          <profiles>
            <profile id="3f8b-bc33-8f7e-39d8" name="Cáliz de Malfleur" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">Declara su uso al inicio de la fase de magia del oponente. Lanza 1D6: con un 1 sufre una Herida sin posibilidad de efectuar tiradas de salvación por armadura ni especiales. Con un 2-6, añade un dado adicional de dispersión a la reserva de dados de dispersión del ejército.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="20.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="7a4e-88e5-81ee-32d8" name="Poción sacra" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="fc59-71e7-36b4-b884" type="max"/>
