        <profile id="4cc1-b0c1-ded4-d055" name="Arca de las almas" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd"/>
