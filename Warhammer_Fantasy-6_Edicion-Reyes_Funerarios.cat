                        <selectionEntry id="f137-bc75-1751-2d92" name="¡Montones de pistolas!" hidden="false" collective="true" import="true" type="upgrade">
                          <constraints>
                            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="920e-cfb1-622b-22f6" type="max"/>
